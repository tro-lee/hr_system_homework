generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Employee {
  id         Int      @id @default(autoincrement())
  name       String
  department String
  position   String
  email      String   @unique
  employeeId String   @unique
  hireDate   DateTime
  phone      String
}